;
; BIND data file for local loopback interface
;
$TTL 604800
@ IN SOA lab.pcfdemo.net. root.lab.pcfdemo.net. (
        2; Serial
   604800; Refresh
    86400; Retry
  2419200; Expire
 604800 ); Negative Cache TTL
;
@ IN NS lab.pcfdemo.net.
@ IN A       192.168.0.100
* IN A       192.168.0.100
jumpbox IN A       192.168.0.10
vcenter IN A       192.168.0.10
avi IN A       192.168.0.15
test IN A       192.168.0.50
