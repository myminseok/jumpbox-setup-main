;
; BIND data file for local loopback interface
;
$TTL 604800
@ IN SOA lab.pcfdemo.net. root.lab.pcfdemo.net. (
        2; Serial
   604800; Refresh
    86400; Retry
  2419200; Expire
 604800 ); Negative Cache TTL
;
@ IN NS lab.pcfdemo.net.
@ IN A       192.168.0.6
* IN A       192.168.0.6
jumpbox IN A       192.168.0.6
vcenter IN A       192.168.0.10
infra-harbor IN A       192.168.0.6
ldapserver IN A       192.168.0.6
infra-harbor2 IN A       192.168.0.6
keyclock IN A       192.168.0.6
gitlab IN A       192.168.0.6
avi IN A       192.168.0.16
*.apps IN A 192.168.0.70
*.sys IN A 192.168.0.70
uaa.sys IN A 192.168.0.70
login.sys IN A 192.168.0.70
tpsm IN A       192.168.0.6
